{
  "name": "api-query-params",
  "version": "6.0.1",
  "description": "Convert query parameters from API urls to MongoDB queries",
  "main": "src/index.js",
  "types": "types.d.ts",
  "type": "module",
  "files": [
    "src/",
    "types.d.ts"
  ],
  "scripts": {
    "coverage": "nyc npm test && nyc report --reporter=html",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
